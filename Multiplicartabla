#include <iostream>

int main() {
    int numero;

    // Solicitar al usuario la tabla de multiplicar
    std::cout << "Ingrese un nÃºmero para obtener su tabla de multiplicar: ";
    std::cin >> numero;

    // Imprimir la tabla de multiplicar del 1 al 12
    for (int i = 1; i <= 12; ++i) {
        std::cout << numero << " x " << i << " = " << numero * i << std::endl;
    }

    return 0;
}

